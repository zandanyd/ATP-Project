<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import View.MazeDisplay?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MyViewController">
    <children>
        <MazeDisplay fx:id="mazeDisplay" height="600.0" width="600.0"
